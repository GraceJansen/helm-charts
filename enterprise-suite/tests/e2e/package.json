{
  "name": "es-console-spa-e2e",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.11"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run start:dev",
    "start:dev": "npm run prestart && webpack-dev-server --port=4200 --open --config webpack.dev.js",
    "start:dev-simple": "npm run prestart && webpack-dev-server --port=4200 --config webpack.dev.js",
    "start:gke": "npm run prestart && webpack-dev-server --port=4200 --open --config webpack.gke.js",
    "start:gke-simple": "npm run prestart && webpack-dev-server --port=4200 --config webpack.gke.js",
    "start:local": "npm run prestart && webpack-dev-server --port=4200 --open --config webpack.local.js",
    "start:local-simple": "npm run prestart && webpack-dev-server --port=4200 --config webpack.local.js",
    "start:dev-release": "npm run prestart && webpack-dev-server --port=4200 --open --config webpack.dev-release.js",
    "start:dev-release-simple": "npm run prestart && webpack-dev-server --port=4200 --config webpack.dev-release.js",
    "start:prod": "webpack-dev-server --port=4200 --open --config webpack.prod.js",
    "typecheck-app": "tsc --noEmit -p src/tsconfig.app.json",
    "typecheck-test": "tsc --noEmit -p src/tsconfig.spec.json",
    "typecheck": "run-s typecheck-app typecheck-test",
    "build": "npm run build:prod",
    "build:prod": "npm run prebuild && webpack --config webpack.prod.js && npm run versionindex",
    "build:dev": "npm run prebuild && webpack --config webpack.dev.js",
    "build:dev-release": "npm run prebuild && webpack --config webpack.dev-release.js",
    "inspect": "DEBUG=true node --inspect --debug-brk node_modules/.bin/webpack",
    "test": "karma start ./karma.conf.js",
    "test:no-watch": "karma start ./karma.conf.js --single-run",
    "test-headless": "karma start ./karma.conf.js --browsers ChromeHeadless --single-run",
    "test-doc": "ts-node node_modules/.bin/jasmine '**/*.doc.ts'",
    "lint": "ng lint",
    "lint-preview": "npm run lint-staged HEAD~ --",
    "patch-type": "cp -a typings/* node_modules/@types/",
    "prom-forward": "kubectl port-forward -n prometheus $(kubectl get -n prometheus pod -l 'app=prometheus,component=server' -o jsonpath='{.items[0].metadata.name}' --namespace prometheus) 9090",
    "stylelint": "stylelint \"./src/**/*.css\"",
    "e2e": "cypress run",
    "e2e:minikube": "cypress run --env configFile=minikube",
    "e2e:travis-setup:stable": "./scripts/travis_e2e_setup.sh stable",
    "e2e:travis-setup:latest": "./scripts/travis_e2e_setup.sh latest",
    "e2e:travis-prs": "NO_COLOR=1 cypress run --env configFile=minikube,skipKnownError=true",
    "e2e:travis-prs:result": "NO_COLOR=1 cypress run --env configFile=minikube,skipKnownError=true | tee results.txt",
    "e2e:check-results": "./scripts/check_e2e_results.sh",
    "e2e:patch-minikube-ip": "./scripts/patch-minikube-ip.sh",
    "e2e:travis": "cypress run --env configFile=minikube,skipKnownError=true",
    "e2e:demo-app-setup": "kubectl apply -f ./scripts/es-demo.yaml",
    "cypress:open": "cypress open",
    "cypress:minikube:open": "cypress open --env configFile=minikube",
    "cypress:minikube:skip-error:open": "cypress open --env configFile=minikube,skipKnownError=true",
    "setup-clean-es2:latest": "./scripts/setup_clean_es2_latest.sh",
    "setup-clean-es2:release": "./scripts/setup_clean_es2_release.sh",
    "prometheus:reset": "./scripts/reset-prometheus.sh",
    "docker-image:dev-release": "npm run build:dev-release && docker build -t lightbend-docker-registry.bintray.io/enterprise-suite/es-console:e2e .",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "docserver": "ts-node-dev --no-deps --no-notify scripts/docserver.ts",
    "install-deps": "npm install",
    "packageversion": "echo $npm_package_version",
    "versionindex": "sh scripts/add_build_info.sh",
    "uninstall-prom-client": "rm -rf node_modules/@lightbend/es-prometheus-client",
    "reinstall-prom-client": "run-s uninstall-prom-client install-deps"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/store": "7.0.2",
